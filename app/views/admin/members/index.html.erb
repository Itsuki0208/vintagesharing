<div class='row'>
    <div class= "col-md-8 offset-md-1">
      <h2>Member Information</h2><br />
        <table class= 'table table hover'>
          <thead>
            <tr>
              <th>会員ID</th>
              <th>ニックネーム</th>
              <th>性別</th>
              <th>年代</th>
            </tr>
          </thead>
          <tbody>
            <% @members.each do |member| %>
              <tr>
                <td><%= member.id%></td>
                <td><%= member.nick_name %></td>
                <td><%= member.gender %></td>
                <td><%= member.age %></td>
                <!--年代を年齢で文字列で表示させたい-->
              </tr>
            <% end %>
          </tbody>
        </table>
    </div>
    
  </div>